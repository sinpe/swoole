{
    "name": "long/swoole",
    "description": "基于Swoole的框架",
    "keywords": ["framework","swoole","api","router"],
    "homepage": "https://github.com/sinpe/swoole",
    "type": "framework",
    "license": "MIT",
    "authors": [
        {
		    "name": "Sinpe",
		    "email": "18222544@qq.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "psr/http-message": "^1.0",
        "illuminate/support": "^5.6",
        "long/support": "@dev",
        "long/route": "dev-master",
        "long/middleware": "dev-master",
	"long/event": "dev-master",
        "psr/log": "^1.0",
        "long/ioc": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "provide": {
        "psr/http-message-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Sinpe\\Swoole\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
